{
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-xml": "*",
        "anankke/omnipay-alipay": "^3.1.3",
        "aws/aws-sdk-php": "^3",
        "cloudflare/sdk": "^1",
        "esdeathlove/datatables": "^1.6",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/database": "^9.0",
        "illuminate/pagination": "^9.0",
        "irazasyed/telegram-bot-sdk": "^3",
        "khanamiryan/qrcode-detector-decoder": "*",
        "league/html-to-markdown": "^5.1",
        "league/omnipay": "^3.2.1",
        "mailgun/mailgun-php": "^3",
        "paymentwall/paymentwall-php": "^2",
        "phpmailer/phpmailer": "^6",
        "ramsey/uuid": "^4",
        "robmorgan/phinx": "^0.13",
        "sendgrid/sendgrid": "^8",
        "sentry/sentry": "^3",
        "slim/slim": "^3.12",
        "smarty/smarty": "^4",
        "stripe/stripe-php": "^7",
        "symfony/console": "*",
        "symfony/yaml": "^6",
        "voku/anti-xss": "^4",
        "zeuxisoo/slim-whoops": "0.6.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "nunomaduro/phpinsights": "*"
    }
}
